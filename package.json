{
  "name": "js2-ca",
  "version": "0.0.1",
  "description": "Building a Social media platform",
  "scripts": {
    "docs": "jsdoc -c ./conf.json -r",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.html\" \"src/**/*.css\"",
    "lint": "eslint  \"src/components/**/*.js\" \"src/utils/**/*.js\"",
    "prepare": "husky install",
    "test": "npm run test-unit",
    "test-unit": "jest",
    "test_cypress": "npm run test-e2e-cli",
    "test_cypress-e2e": "cypress open",
    "test_cypress-e2e-report": "cypress run --reporter mochawesome",
    "test_cypress-e2e-cli": "cypress run"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnaCascante/2023-10-15_JS2_CA_Ana-Cascante-Jean-Christoffer-Dahl_Repo.git"
  },
  "keywords": [
    "CSS",
    "CSS",
    "Frameworks",
    "Bootstrap"
  ],
  "author": "Ana Cascante <anacas71213@stud.noroff.no> and Jean Christoffer Dahl <JeaDah78334@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnaCascante/2023-10-15_JS2_CA_Ana-Cascante-Jean-Christoffer-Dahl_Repo.git/issues"
  },
  "homepage": "https://github.com/AnaCascante/2023-10-15_JS2_CA_Ana-Cascante-Jean-Christoffer-Dahl_Repo.git#readme",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.3.3",
    "eslint": "^8.52.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3"
  }
}
